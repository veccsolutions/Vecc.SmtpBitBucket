CREATE TABLE IF NOT EXISTS "Identity"."UserClaims"(
	"Id" SERIAL NOT NULL,
	"UserId" INT NOT NULL,
	"ClaimType" TEXT NULL,
	"ClaimValue" TEXT NULL,

    CONSTRAINT "PK_UserClaims" PRIMARY KEY ("Id")
);

CREATE INDEX IF NOT EXISTS "IX_UserClaims_UserId" ON "Identity"."UserClaims" ("UserId" ASC);
