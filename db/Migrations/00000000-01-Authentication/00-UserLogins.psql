ALTER TABLE "Identity"."UserLogins" ADD CONSTRAINT "PK_UserLogins" PRIMARY KEY ("LoginProvider", "ProviderKey");

CREATE INDEX IF NOT EXISTS "IX_UserLogins_UserId" ON "Identity"."UserLogins" ("UserId" ASC);