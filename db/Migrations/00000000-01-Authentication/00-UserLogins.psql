CREATE TABLE IF NOT EXISTS "Identity"."UserLogins"
(
	"LoginProvider" VARCHAR(128) NOT NULL,
	"ProviderKey" VARCHAR(128) NOT NULL,
	"ProviderDisplayName" TEXT NULL,
	"UserId" INT NOT NULL
